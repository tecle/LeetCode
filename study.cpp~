void printNum(int num){
	int i=31;
	char a[32];
	while(i>=0){
		a[i]=(num&1)+'0';
		num=num>>1;
		i--;
	}
	for(i=0;i<32;i++)
		cout<<a[i];
	cout<<endl;
}
int main(){

	int x=INT_MIN;
